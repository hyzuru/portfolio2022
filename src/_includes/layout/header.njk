    <div class="l-header__block">
      <div class="l-header__logo">
        <a href="{{ site.root }}">
          <img src="{{ site.root }}assets/images/header_logo.svg" width="300" height="71" alt="Green Meat&trade; 健康と地球とずっと。">
        </a>
      </div>
      <button type="button" class="l-header__button js-openButton" aria-controls="drawer" aria-expanded="false" aria-label="ナビゲーションを開く">
        <span lang="en" class="l-header__menu u-uppercase">menu</span>
        <span class="l-header__dot-wrapper">
          <span class="c-dot c-dot--header"></span>
        </span>
